{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/templates/trainer.html"
        }
        
    ],
    "headers": [
        {
            "source": "/audio/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}