{
    "builds": [
        {
            "src": "api/server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "^/static/(.*)$",
            "dest": "/static/$1"
        },
        {
            "src": "/",
            "dest": "api/server.py"
        }
    ]
}